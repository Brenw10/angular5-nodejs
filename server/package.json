{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "nodemon ./bin/www --exec babel-node",
    "clean": "rm -rf .tmp && rm -rf dist",
    "build": "npm run clean && babel . ./bin/www -s -d .tmp --ignore node_modules",
    "serve-build": "npm run build && node ./.tmp/bin/www"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "express": "~4.15.5",
    "mongodb": "^3.0.3",
    "mongoose": "^5.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "nodemon": "^1.17.1"
  }
}